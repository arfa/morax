{
  "name": "nextjs-commerce",
  "version": "1.0.0",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "test": "jest --watch",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "find:unused": "npx next-unused",
    "generate": "graphql-codegen",
    "generate:shopify": "DOTENV_CONFIG_PATH=./.env.local graphql-codegen -r dotenv/config --config framework/shopify/codegen.json",
    "generate:vendure": "graphql-codegen --config framework/vendure/codegen.json",
    "generate:definitions": "node framework/bigcommerce/scripts/generate-definitions.js",
    "check:prettier": "prettier --check \"./{components, pages}/**/*.{js,jsx,ts,tsx,css,md,mdx,json}\"",
    "check:lint": "eslint \"./{components, pages}/**/*.{js,jsx,ts,tsx}\"",
    "check:type": "tsc --project tsconfig.json --pretty --noEmit",
    "fix:prettier": "prettier --write \"./{components, pages}/**/*.*.{js,jsx,ts,tsx,css,md,mdx,json}\"",
    "fix:lint": "next lint --fix --no-cache",
    "fix:all": "npm run fix:lint && npm run fix:prettier",
    "postinstall": "husky install",
    "commit": "git-cz",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s public"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "sideEffects": false,
  "license": "MIT",
  "engines": {
    "node": ">=14.x"
  },
  "dependencies": {
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@mui/lab": "5.0.0-alpha.60",
    "@mui/material": "5.4.1",
    "@react-spring/web": "9.4.3",
    "@storybook/addon-docs": "6.4.19",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.2",
    "@types/autosuggest-highlight": "3.2.0",
    "@types/react-swipeable-views-utils": "0.13.3",
    "@vercel/fetch": "6.1.1",
    "autoprefixer": "10.4.2",
    "autosuggest-highlight": "3.2.0",
    "axios": "0.25.0",
    "body-scroll-lock": "3.1.5",
    "classnames": "2.3.1",
    "cookie": "0.4.1",
    "cross-env": "7.0.3",
    "email-validator": "2.0.4",
    "immutability-helper": "3.1.1",
    "jest": "27.5.1",
    "js-cookie": "2.2.1",
    "keen-slider": "6.6.3",
    "lodash.debounce": "4.0.8",
    "lodash.random": "3.2.0",
    "lodash.throttle": "4.1.1",
    "next": "12.0.10",
    "next-seo": "4.28.1",
    "next-sitemap": "1.9.9",
    "next-themes": "0.0.14",
    "postcss": "8.4.6",
    "postcss-nesting": "8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-fast-marquee": "1.3.1",
    "react-icons": "4.3.1",
    "react-merge-refs": "1.1.0",
    "react-use-measure": "2.1.1",
    "stripe": "8.202.0",
    "swell-js": "4.0.0-next.0",
    "swr": "0.5.6",
    "tabbable": "5.2.1",
    "ts-jest": "27.1.3",
    "uuidv4": "6.2.12"
  },
  "devDependencies": {
    "@babel/core": "7.17.2",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/schema-ast": "1.18.3",
    "@graphql-codegen/typescript": "1.22.2",
    "@graphql-codegen/typescript-operations": "1.18.1",
    "@next/bundle-analyzer": "10.2.3",
    "@storybook/addon-actions": "6.4.19",
    "@storybook/addon-essentials": "6.4.19",
    "@storybook/addon-links": "6.4.19",
    "@storybook/builder-webpack5": "6.4.19",
    "@storybook/manager-webpack5": "6.4.19",
    "@storybook/react": "6.4.19",
    "@types/body-scroll-lock": "2.6.1",
    "@types/cookie": "0.4.0",
    "@types/js-cookie": "2.2.6",
    "@types/lodash.debounce": "4.0.6",
    "@types/lodash.random": "3.2.6",
    "@types/lodash.throttle": "4.1.6",
    "@types/node": "15.12.4",
    "@types/react": "17.0.39",
    "@types/react-swipeable-views": "0.13.1",
    "babel-loader": "8.2.3",
    "chromatic": "6.4.3",
    "commitizen": "4.2.4",
    "deepmerge": "4.2.2",
    "eslint": "7.23.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-unused-imports": "1.0.0",
    "graphql": "15.5.1",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.5.1",
    "storybook-dark-mode": "1.0.8",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript": "4.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  }
}
